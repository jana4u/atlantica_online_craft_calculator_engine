<% if @item %>
  $('#general').html('<%= js_escape_html(partial('general')) %>');
  $('#my').html('<%= js_escape_html(partial('my')) %>');
<% else %>
  $('#item_name').replaceWith('<%= js_escape_html(items_select_tag) %>');
  $("#item_name").change(function() {
  if ($(this).prop("selectedIndex") > 0) {
  $(this).closest("form").submit();
  }
  });
<% end %>
try
{
_gaq.push(['_trackPageview', '<%= request.fullpath.gsub(/(&|\?)_=[0-9]*/, '') %>']);
}
catch(err)
{
}
