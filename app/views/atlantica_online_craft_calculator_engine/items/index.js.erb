<% if @item %>
  $('#general').html('<%= j(render partial: 'general') %>');
  $('#my').html('<%= j(render partial: 'my') %>');
<% else %>
  $('#item_name').replaceWith('<%= j(items_select_tag) %>');
  $("#item_name").change(function() {
  if ($(this).prop("selectedIndex") > 0) {
  $(this).closest("form").submit();
  }
  });
<% end %>
try
{
_gaq.push(['_trackPageview', '<%= request.fullpath.gsub(/(&|\?)_=[0-9]*/, '') %>']);
}
catch(err)
{
}
